<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="subsystem" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="jboss-op">
    <elem name="name" value="add"/>
    <elem name="addr-params" value="host,profile,subsystem"/>
    <elem name="op-params"/>
  </annotation>
  <refs>
    <ref feature="extension" include="true"/>
    <ref feature="host" nillable="true"/>
    <ref feature="profile" include="true" nillable="true"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="extension" nillable="false"/>
  </params>
</feature-spec>
