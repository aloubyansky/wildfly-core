<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="profile" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="jboss-op">
    <elem name="name" value="add"/>
    <elem name="addr-params" value="host,profile"/>
    <elem name="skip-if-filtered" value="profile"/>
  </annotation>
  <refs>
    <ref feature="host" nillable="true"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
  </params>
</feature-spec>
