<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="audit-logger" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,core-service,access,logger"/>
  </annotation>
  <refs>
    <ref feature="audit"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="core-service" default="management" feature-id="true"/>
    <param name="access" feature-id="true"/>
    <param name="logger" default="audit-log" feature-id="true"/>
    <param name="log-boot" default="true"/>
    <param name="log-read-only" default="false"/>
    <param name="enabled" default="false"/>
  </params>
</feature-spec>
