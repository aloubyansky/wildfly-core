<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="elytron-constant-permission-mapper-permission" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="jboss-op">
    <elem name="name" value="list-add"/>
    <elem name="addr-params" value="host,profile,subsystem,constant-permission-mapper"/>
    <elem name="op-params" value="permissions"/>
  </annotation>
  <refs>
    <ref feature="elytron-constant-permission-mapper"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="constant-permission-mapper" default="constant-permission-mapper" feature-id="true"/>
    <param name="permissions" feature-id="true"/>
  </params>
</feature-spec>
