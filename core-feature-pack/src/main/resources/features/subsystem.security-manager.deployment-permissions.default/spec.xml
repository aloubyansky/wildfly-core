<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="subsystem.security-manager.deployment-permissions.default" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,deployment-permissions"/>
    <elem name="op-params" value="maximum-permissions"/>
  </annotation>
  <refs>
    <ref feature="subsystem.security-manager"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="deployment-permissions" default="default" feature-id="true"/>
    <param name="maximum-permissions"/>
  </params>
</feature-spec>
