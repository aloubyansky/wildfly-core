<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="elytron-logical-permission-mapper" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,logical-permission-mapper"/>
  </annotation>
  <provides>
    <capability name="$profile.org.wildfly.security.permission-mapper.$logical-permission-mapper"/>
  </provides>
  <requires>
    <capability name="$profile.org.wildfly.security.permission-mapper.$left"/>
    <capability name="$profile.org.wildfly.security.permission-mapper.$right"/>
  </requires>
  <refs>
    <ref feature="elytron"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="logical-permission-mapper" feature-id="true"/>
    <param name="logical-operation"/>
    <param name="left"/>
    <param name="right"/>
  </params>
</feature-spec>
