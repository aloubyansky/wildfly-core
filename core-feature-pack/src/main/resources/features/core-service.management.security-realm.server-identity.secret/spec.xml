<?xml version="1.0" ?>

<feature-spec xmlns="urn:wildfly:pm-feature-spec:1.0" name="core-service.management.security-realm.server-identity.secret">
    <annotation name="add">
        <elem name="op-params" value="credential-reference,value"/>
        <elem name="addr-params" value="host,core-service,security-realm,server-identity"/>
    </annotation>
    <refs>
        <ref feature="core-service.management"/>
        <ref feature="core-service.management.security-realm"/>
        <ref feature="host" nillable="true"/>
    </refs>
    <params>
        <param name="host" default="PM_UNDEFINED" feature-id="true"/>
        <param name="server-identity" feature-id="true" default="secret"/>
        <param name="security-realm" feature-id="true"/>
        <param name="credential-reference" nillable="true"/>
        <param name="value" nillable="true"/>
        <param name="core-service" feature-id="true" default="management"/>
    </params>
</feature-spec>
