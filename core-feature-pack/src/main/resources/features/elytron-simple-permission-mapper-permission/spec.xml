<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="elytron-simple-permission-mapper-permission" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="list-add">
    <elem name="addr-params" value="host,profile,subsystem,simple-permission-mapper"/>
    <elem name="op-params" value="permission-mapping"/>
    <elem name="op-params-mapping" value="permission-mappings"/>
  </annotation>
  <refs>
    <ref feature="elytron"/>
    <ref feature="elytron-simple-permission-mapper"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="simple-permission-mapper" feature-id="true"/>
    <param name="permission-mapping" feature-id="true"/>
  </params>
</feature-spec>
