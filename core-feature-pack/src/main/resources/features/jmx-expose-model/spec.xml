<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="jmx-expose-model" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="jboss-op">
    <elem name="name" value="add"/>
    <elem name="addr-params" value="host,profile,subsystem,expose-model"/>
    <elem name="op-params"/>
  </annotation>
  <refs>
    <ref feature="jmx"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="expose-model" feature-id="true"/>
  </params>
</feature-spec>
