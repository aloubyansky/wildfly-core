<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="elytron-provider" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="jboss-op">
    <elem name="name" value="add"/>
    <elem name="addr-params" value="host,profile,subsystem,provider-loader"/>
    <elem name="op-params" value="module"/>
  </annotation>
  <refs>
    <ref feature="elytron"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="provider-loader" feature-id="true"/>
    <param name="module" nillable="false"/>
  </params>
</feature-spec>
