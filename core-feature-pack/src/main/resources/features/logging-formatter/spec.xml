<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="logging-formatter" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,pattern-formatter"/>
    <elem name="op-params" value="pattern"/>
  </annotation>
  <refs>
    <ref feature="subsystem"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="pattern-formatter" feature-id="true"/>
    <param name="pattern"/>
  </params>
</feature-spec>
