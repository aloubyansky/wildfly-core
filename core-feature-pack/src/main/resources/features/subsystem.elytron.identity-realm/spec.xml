<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="subsystem.elytron.identity-realm" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,identity-realm"/>
    <elem name="op-params" value="identity"/>
  </annotation>
  <refs>
    <ref feature="subsystem.elytron"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="identity-realm" feature-id="true"/>
    <param name="identity" nillable="false"/>
  </params>
</feature-spec>
