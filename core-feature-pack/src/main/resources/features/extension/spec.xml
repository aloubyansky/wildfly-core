<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="extension" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,extension"/>
    <elem name="op-params"/>
  </annotation>
  <refs>
    <ref feature="host" nillable="true"/>
  </refs>
  <params>
    <param name="extension" feature-id="true"/>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
  </params>
</feature-spec>
