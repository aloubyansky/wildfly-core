<?xml version="1.0" encoding="UTF-8"?>
<feature-spec name="logger" xmlns="urn:wildfly:pm-feature-spec:1.0">
  <annotation name="add">
    <elem name="addr-params" value="host,profile,subsystem,logger"/>
    <elem name="op-params" value="level"/>
  </annotation>
  <refs>
    <ref feature="logging"/>
  </refs>
  <params>
    <param name="host" default="PM_UNDEFINED" feature-id="true"/>
    <param name="profile" default="PM_UNDEFINED" feature-id="true"/>
    <param name="subsystem" feature-id="true"/>
    <param name="logger" feature-id="true"/>
    <param name="level" nillable="false"/>
  </params>
  <packages>
    <package name="org.jboss.as.logging.main"/>
  </packages>
</feature-spec>
